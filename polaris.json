{
  "version": 1,
  "projects": {
    "elasticity-strategies": {
      "projectType": "library",
      "tsEntryPoint": "libs/elasticity-strategies/src/index.ts",
      "importPath": "@nicokratky/elasticity-strategies",
      "crds": {
        "tsConfig": "tsconfig.base.json",
        "outDir": "libs/elasticity-strategies/crds",
        "polarisTypes": [
          "HorizontalVerticalElasticityStrategy",
          "HorizontalElasticityStrategy"
        ]
      }
    },
    "horizontal-vertical-elasticity-strategy-controller": {
      "projectType": "elasticity-strategy-controller",
      "tsEntryPoint": "apps/horizontal-vertical-elasticity-strategy-controller/src/main.ts"
    },
    "horizontal-elasticity-strategy-controller": {
      "projectType": "elasticity-strategy-controller",
      "tsEntryPoint": "apps/horizontal-elasticity-strategy-controller/src/main.ts"
    },
    "slos": {
      "projectType": "library",
      "tsEntryPoint": "libs/slos/src/index.ts",
      "importPath": "@nicokratky/slos",
      "crds": {
        "tsConfig": "tsconfig.base.json",
        "outDir": "libs/slos/crds",
        "polarisTypes": ["EfficiencyMetricMapping", "EfficiencySloMapping"]
      }
    },
    "reactive-efficiency": {
      "projectType": "composed-metric-controller",
      "tsEntryPoint": "apps/reactive-efficiency/src/main.ts"
    },
    "efficiency-controller": {
      "projectType": "slo-controller",
      "tsEntryPoint": "apps/efficiency-controller/src/main.ts"
    }
